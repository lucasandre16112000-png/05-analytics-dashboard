# Configurações do Analytics Dashboard

# Diretório de dados de entrada
DATA_DIR=./data

# Arquivo de dados de entrada
INPUT_DATA_FILE=sample_data.json

# Diretório de saída
OUTPUT_DIR=./output

# Arquivo de relatório JSON
OUTPUT_REPORT_FILE=analytics_report.json

# Arquivo HTML do dashboard
OUTPUT_DASHBOARD_FILE=dashboard.html

# Nível de log (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
